{
	"env": {},
	"permissions": {
		"allow": [],
		"deny": []
	},
	"hooks": {
		"SessionStart": [
			{
				"hooks": [
					{
						"type": "command",
						"command": "node \"$HOME/.claude/hooks/gsd-check-update.js\""
					}
				]
			}
		]
	},
	"statusLine": {
		"type": "command",
		"command": "node \"/Users/tensor/.claude/hooks/gsd-statusline.js\""
	},
	"enabledPlugins": {
		"code@InSuperposition": true,
		"git@InSuperposition": true,
		"reasoning@InSuperposition": true,
		"javascript@InSuperposition": true,
		"playwright@claude-plugins-official": true,
		"code-simplifier@claude-plugins-official": true,
		"plugin-dev@claude-plugins-official": true,
		"rust-analyzer-lsp@claude-plugins-official": true,
		"jdtls-lsp@claude-plugins-official": true
	},
	"alwaysThinkingEnabled": true
}
